  "events": {
    "load": [
      {
        "name": "c4g.getEquipamentoByid_recursos",
        "parameters": [
          {
            "name": "id_recursos",
            "value": "${parameters.id_recursos}"
          }
        ],
        "then": [
          {
            "name": "equipamento",
            "type": "set",
            "value": "${result}",
            "valueType": "C4G.Models.C4G.Equipamento"
          }
        ],
        "error": [],
        "type": "invoke"
      },
      {
        "name": "c4g.getLaboratorios",
        "parameters": [],
        "then": [
          {
            "name": "getLaboratoriosResult",
            "type": "set",
            "value": "${result}",
            "valueType": "IEnumerable<C4G.Models.C4G.Laboratorio>"
          }
        ],
        "type": "invoke"
      },
      {
        "name": "c4g.getRecursos",
        "parameters": [],
        "then": [
          {
            "name": "getRecursosResult",
            "type": "set",
            "value": "${result}",
            "valueType": "IEnumerable<C4G.Models.C4G.Recurso>"
          }
        ],
        "type": "invoke"
      }
    ]
  },
