                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Laboratorio",
                              "component": "id_lab",
                              "name": "id_labLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "data": "${getLaboratoriosResult}",
                              "textProperty": "designacao",
                              "valueProperty": "id_lab",
                              "placeholder": "Choose Laboratorio",
                              "valueType": "int",
                              "type": "dropdown",
                              "name": "id_lab",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${produto.id_lab}"
                            },
                            {
                              "type": "required-validator",
                              "name": "id_labRequiredValidator",
                              "component": "id_lab",
                              "text": "id_lab is required",
                              "style": {
                                "position": "absolute"
                              },
                              "defaultValue": "0"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
