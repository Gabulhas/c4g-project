                                "position": "absolute"
                              },
                              "defaultValue": "0"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "components": [
                        {
                          "type": "col",
                          "class": "col offset-sm-3",
                          "components": [
                            {
                              "type": "button",
                              "buttonType": "submit",
                              "icon": "save",
                              "text": "Save",
                              "buttonStyle": "primary"
                            },
                            {
                              "type": "button",
                              "buttonStyle": "light",
                              "text": "Cancel",
                              "events": {
                                "click": [
                                  {
                                    "type": "close-popup"
                                  }
                                ]
                              }
                            }
                          ]
                        }
                      ]
                    }
                  ],
                  "events": {
                    "submit": [
                      {
                        "error": [
                          {
                            "detail": "Unable to update Produto",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "c4g.updateProduto",
                        "parameters": [
                          {
                            "name": "id_recursos",
                            "value": "${parameters.id_recursos}"
                          },
                          {
                            "name": "Produto",
                            "value": "${produto}"
                          }
                        ],
                        "then": [
                          {
                            "type": "close-popup",
                            "result": "${produto}"
                          }
                        ],
                        "type": "invoke"
                      }
                    ]
                  },
                  "itemType": "C4G.Models.C4G.Produto",
                  "data": "${produto}",
                  "visible": "${produto != null}"
                }
              ]
            }
          ]
        }
      ]
    }
  ],
  "events": {
    "load": [
      {
        "name": "c4g.getProdutoByid_recursos",
        "parameters": [
          {
            "name": "id_recursos",
            "value": "${parameters.id_recursos}"
          }
        ],
        "then": [
          {
            "name": "produto",
            "type": "set",
            "value": "${result}",
            "valueType": "C4G.Models.C4G.Produto"
          }
        ],
        "error": [],
        "type": "invoke"
      },
      {
        "name": "c4g.getLaboratorios",
        "parameters": [],
        "then": [
          {
            "name": "getLaboratoriosResult",
            "type": "set",
            "value": "${result}",
            "valueType": "IEnumerable<C4G.Models.C4G.Laboratorio>"
          }
        ],
        "type": "invoke"
      },
      {
        "name": "c4g.getRecursos",
        "parameters": [],
        "then": [
          {
            "name": "getRecursosResult",
            "type": "set",
            "value": "${result}",
            "valueType": "IEnumerable<C4G.Models.C4G.Recurso>"
          }
        ],
        "type": "invoke"
      }
    ]
  },
  "name": "Edit Produto",
  "access": [
    "Authenticated"
  ],
  "layout": "Main"
}